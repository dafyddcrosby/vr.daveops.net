!!! 5
%html
  %head
    %meta{charset: "utf-8"}
    %meta{name: "description", content: "VR demo"}
    %title VR stuff...
    %script{src: "https://aframe.io/releases/1.1.0/aframe.min.js"}
    %script{src: "https://cdn.rawgit.com/donmccurdy/aframe-extras/v6.1.0/dist/aframe-extras.min.js"}
  %body
    %a-entity{"oculus-go-controls": true}
    %a-sky{color: "#6EBAA7"}
    %a-scene{background: "color: black",
      "keyboard-shortcuts": "enterVR: true",
      "vr-mode-ui": "enabled: true",
      stats: false}
      %a-plane{position: "0 0 0", rotation: "-90 0 0", width: "12.1285", height: "20", color: "gray"}
      %a-plane{position: "1 0 0", rotation: "0 -90 90", width: "3", height: "2", color: "red"}
      %a-plane{position: "-1 0 0", rotation: "0 -90 90", width: "3", height: "2", color: "red"}
      %a-link{href:"soda.html"}
